<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite">

	<test thread-count="5" name="Test_Valid">
		<!-- Add browser name(chrome/firefox/edge/ie/safari) in value attribue -->
		<parameter name="browser" value="chrome"></parameter>

		<classes>
			<class name="Testpgs.TC_LoginPage" />
		</classes>
	</test> <!-- Test -->

	<test thread-count="5" name="Test_Invalid">
